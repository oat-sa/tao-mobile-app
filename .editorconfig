; .editorconfig

root = true

;[*]
;indent_style = space
;indent_size = 4
;charset = utf-8
;insert_final_newline = true

[**.js]
end_of_line = lf
trim_trailing_whitespace = true

[**.json]
indent_size = 2
end_of_line = lf
trim_trailing_whitespace = true

[**.{html,tpl}]
brace_style = expand
end_of_line = lf
trim_trailing_whitespace = true

